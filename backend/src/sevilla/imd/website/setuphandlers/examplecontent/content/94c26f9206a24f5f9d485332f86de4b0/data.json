              {
                "key": "1h268",
                "type": "data",
                "value": [
                  {
                    "children": [
                      {
                        "text": "DNI/NIF, nombre y apellidos, direcci\u00f3n postal, direcci\u00f3n electr\u00f3nica, tel\u00e9fono, firma electr\u00f3nica, imagen, otros datos, caracter\u00edsticas personales, circunstancias sociales, detalles de empleo, datos econ\u00f3micos"
                      }
                    ],
                    "type": "p"
                  }
                ]
              }
